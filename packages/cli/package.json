{
  "name": "@subnetter/cli",
  "version": "2.2.0",
  "description": "Command-line interface for Subnetter",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "subnetter": "./bin/subnetter.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc && yarn build:bin",
    "build:bin": "node scripts/create-bin.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "cd ../.. && yarn eslint packages/cli/src --fix",
    "clean": "rimraf dist",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "prepare": "cd ../cidr-utils && yarn build && cd ../core && yarn build"
  },
  "dependencies": {
    "@subnetter/cidr-utils": "2.2.0",
    "@subnetter/core": "2.2.0",
    "@subnetter/netbox": "2.2.0",
    "@subnetter/nlp": "2.2.0",
    "chalk": "^4.1.2",
    "commander": "^13.1.0",
    "csv-writer": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "ts-node": "^10.9.2"
  }
}
